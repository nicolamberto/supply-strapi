{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "medidas": {
      "type": "string",
      "required": false
    },
    "caracteristicas": {
      "type": "string"
    },
    "codigo": {
      "type": "uid",
      "required": true,
      "targetField": "nombre"
    },
    "material": {
      "type": "string"
    },
    "capacidad": {
      "type": "string"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "entrada": {
      "type": "string"
    },
    "diametroboca": {
      "type": "string"
    },
    "product_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    }
  }
}
